#ifndef PARAMETER_H_
#define PARAMETER_H_

class Parameter {
public:
	Parameter(std::string value, bool is_string) : value(value), is_string(is_string) {};
	virtual ~Parameter() {};
	bool is_string;
	std::string value;
};

#endif /* PARAMETER_H_ */
