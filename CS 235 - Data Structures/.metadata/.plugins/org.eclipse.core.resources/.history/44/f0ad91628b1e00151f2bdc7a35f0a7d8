#ifndef TOKEN_H_
#define TOKEN_H_

#include <string>

class Token {
public:
	Token();
	virtual ~Token();
private:
	enum Type{COMMA, PERIOD, Q_MARK, LEFT_PAREN, RIGHT_PAREN, COLON, COLON_DASH, SCHEMES, FACTS, RULES, QUERIES, ID, STRING, END};
	std::string value;
	int line;
};

#endif /* TOKEN_H_ */
