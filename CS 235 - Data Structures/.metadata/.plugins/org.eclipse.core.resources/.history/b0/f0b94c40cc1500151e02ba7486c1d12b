#include "Node.h"

Node::Node() {
	height = 0;
	left_child = NULL;
	right_child = NULL;
	data = 0;
}

int Node::getData() {
	return data;
}

Node* Node::getLeftChild() {
	return left_child;
}

Node* Node::getRightChild() {
	return right_child;
}

int Node::getHeight() {
	return height;
}
