#include <sstream>
#include "Student.h"

Student::Student(unsigned long long int id, string name, string address, string tel) {
	this->id = id;
	this->name = name;
}

Student::~Student() {
	// TODO Auto-generated destructor stub
}

unsigned long long int Student::getID() {
	unsigned long long int id;
	return id;
}

string Student::getName() {
	return "";
}

void Student::addGPA(double classGrade) {

}

string Student::toString() {
	stringstream stud;
	stud << this->id << "\n"
			<< this->name << "\n"
			<< this->address << "\n"
			<< this->tel << "\n"
			<< this->gpa;

	return stud.str();
}
